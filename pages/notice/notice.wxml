<block wx:for="{{notice}}" wx:for-item="notice" wx:key>
<view class="cu-card article">
  <view class="cu-item shadow">
      <view class="title">
        <view class="text-cut">{{notice.title}}</view>
      </view>
      <view class="content">
        <image src="{{notice.image}}" mode="aspectFill"></image>
        <view class="desc">
          <view class="text-content"> {{notice.content}}</view>
        </view>
      </view>
    </view>
  </view>
  </block>